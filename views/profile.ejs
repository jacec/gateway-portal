<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Bootswatch: United</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="./css/bootstrap.css" media="screen">
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="../bower_components/html5shiv/dist/html5shiv.js"></script>
      <script src="../bower_components/respond/dest/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <a href="../" class="navbar-brand">Sophrosyne</a>
          <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
        <div class="navbar-collapse collapse" id="navbar-main">
          
          <ul class="nav navbar-nav navbar-right">
            <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown">Jason <b class="caret"></b></a>
                      <ul class="dropdown-menu">
                        <li><a href="#">Refresh Agent</a></li>
                        <li class="divider"></li>
                        <li><a href="#">Settings</a></li>
                        <li><a href="./logout">Logout</a></li>
                      </ul>
                    </li>
          </ul>

        </div>
      </div>
    </div>


    <div class="container">

      <div class="page-header" id="banner">
        <div class="row">
          <div class="col-lg-6">
            <h1>Welcome Jason</h1>
            <p class="lead">Monitoring your network...</p>
          </div>
          
      </div>


      <!-- Tables
      ================================================== -->
      <div class="bs-docs-section">

        <div class="row">
          <div class="col-lg-12">
            <div class="page-header">
              <h1 id="tables">Who's on my network</h1>
            </div>

            <div class="bs-component">
              <table class="table table-striped table-hover ">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Identified Name</th>
                    <th>MAC</th>
                    <th>IP Address</th>
                  </tr>
                </thead>
                <tbody>
                  <% hosts.forEach(function(host){ %>
                  <tr>
                    <td>#</td>
                    <td><%=host.host%></td>
                    <td><%=host.mac%></td>
                    <td><%=host.ip%></td>
                  </tr>
                  <% }); %>
                  
                </tbody>
              </table> 
            </div><!-- /example -->
          </div>
        </div>
      </div>


      <footer>
        <div class="row">
          <div class="col-lg-12">

            <ul class="list-unstyled">
              <li><a href="#">Visit our Blog</a></li>
              <li><a href="#">RSS Feed from your network agent</a></li>
              <li><a href="#">Twitter</a></li>
              <li><a href="#">Contact</a></li>
            </ul>
            
          </div>
        </div>

      </footer>


    </div>


    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
    <script src="./js/bootstrap.min.js"></script>
    <script src="./js/bootswatch.js"></script>
  </body>
</html>
